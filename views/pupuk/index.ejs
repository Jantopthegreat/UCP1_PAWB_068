<h2 style="text-align: center;">Daftar Pupuk</h2>
<table class="pupuk-table">
  <thead>
    <tr>
      <th style="text-align: center;">ID</th>
      <th style="text-align: center;">Nama</th>
      <th style="text-align: center;">Jenis</th>
      <th style="text-align: center;">Komposisi</th>
      <th style="text-align: center;">Kegunaan</th>
      <th style="text-align: center;">Aksi</th>
    </tr>
  </thead>
  <tbody>
    <% pupuk.forEach((p) => { %>
      <tr>
        <td style="text-align: center;"><%= p.id %></td>
        <td style="text-align: center;"><%= p.nama %></td>
        <td style="text-align: center;"><%= p.jenis %></td>
        <td style="text-align: center;"><%= p.komposisi %></td>
        <td style="text-align: center;"><%= p.kegunaan %></td>
        <td style="text-align: center;">
          <a href="/pupuk/edit/<%= p.id %>">Edit</a> |
          <a href="/pupuk/delete/<%= p.id %>" onclick="return confirm('Yakin ingin menghapus?')">Hapus</a>
        </td>
      </tr>
    <% }) %>
  </tbody>
</table>
