<h2 style="text-align: center;">Daftar Tanaman</h2>
<table class="tanaman-table">
  <thead>
    <tr>
      <th>ID</th>
      <th>Nama</th>
      <th>Jenis</th>
      <th>Umur</th>
      <th>Aksi</th>
    </tr>
  </thead>
  <tbody>
    <% tanaman.forEach((t) => { %>
      <tr>
        <td><%= t.id %></td>
        <td><%= t.nama %></td>
        <td><%= t.jenis %></td>
        <td><%= t.umur %></td>
        <td>
          <a href="/tanaman/edit/<%= t.id %>">Edit</a> |
          <a href="/tanaman/delete/<%= t.id %>" onclick="return confirm('Yakin ingin menghapus?')">Hapus</a>
        </td>
      </tr>
    <% }) %>
  </tbody>
</table>
